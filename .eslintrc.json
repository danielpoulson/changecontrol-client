{
	"extends": [
		"plugin:flowtype/recommended",
		"airbnb",
		"prettier",
		"prettier/react"
	],
	"plugins": [
		"flowtype",
		"prettier"
	],
	"parser": "babel-eslint",
	"parserOptions": {
		"ecmaVersion": 2016,
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true
		}
	},
	"rules": {
		"flowtype/no-types-missing-file-annotation": 0,
		"no-underscore-dangle": 0,
		"no-shadow": 0,
		"react/jsx-filename-extension": 0,
		"spaced-comment": 0,
		"react/jsx-no-bind": 0,
		"react/no-array-index-key": 1,
		"jsx-a11y/no-noninteractive-element-interactions": 0,
		"jsx-a11y/no-static-element-interactions": 0,
		"global-require": "off"
	},
	"env": {
		"es6": true,
		"browser": true,
		"node": true,
		"jest": true
	}
}